<div id="search-box" class="container">
    <div class="row">

        <table id="mytable" class="table table-bordered table-striped" cellspacing="0" width="100%">
            <thead>
            <tr>
                <th>Phone</th>

            </tr>
            </thead>
            <tbody>
            {% for data in datas %}
                {% if data.phone != "" %}
                    <tr>
                        <td>{{ data.phone }}</td>
                    </tr>
                {% endif %}
            {% endfor %}
            </tbody>
            <tfoot>
            <tr>

                <th>phone</th>

            </tr>
            </tfoot>
        </table>

    </div>
</div>